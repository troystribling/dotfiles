"*":
  Hydrogen:
    debug: true
  "atom-ide-ui":
    use: {}
  "autocomplete-python":
    useKite: false
  autosave:
    enabled: true
  core:
    disabledPackages: [
      "wrap-guide"
      "linter"
      "linter-solidity"
      "autocomplete-solidity"
      "etheratom"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "desktop.ini"
      "__pycache__"
      "*.pyc"
      ".tags"
      ".ropeproject"
    ]
    openEmptyEditorOnStart: false
    telemetryConsent: "limited"
    themes: [
      "one-light-ui"
      "one-dark-syntax"
    ]
  editor: {}
  "exception-reporting":
    userId: "491ad535-da31-48a5-ae92-ebfdd48bacc8"
  "ide-python":
    pylsPlugins:
      pycodestyle:
        enabled: false
        maxLineLength: 250
      pyflakes:
        enabled: false
  "linter-ui-default":
    panelHeight: 399
  "markdown-writer":
    fileExtension: ".md"
  "split-diff":
    diffWords: true
    ignoreWhitespace: true
    leftEditorColor: "red"
    rightEditorColor: "green"
    scrollSyncType: "Vertical + Horizontal"
  "tool-bar":
    visible: false
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
