root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = false
indent_style = tab
indent_size = 4

[config.cson]
trim_trailing_whitespace = true

[*.{bat,cmd}]
end_of_line = crlf

[*.{yml,yaml}]
indent_style = space

[.git/COMMIT_EDITMSG]
indent_style = space
max_line_length = 72
