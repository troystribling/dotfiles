{
	"arrow_spacing": {
		"level": "error"
	},
	"braces_spacing": {
		"level": "error",
		"spaces": 0
	},
	"colon_assignment_spacing": {
		"level": "error",
		"spacing": {
			"left": 0,
			"right": 1
		}
	},
	"eol_last": {
		"level": "error"
	},
	"indentation": {
		"level": "error",
		"value": 1
	},
	"line_endings": {
		"level": "error"
	},
	"max_line_length": {
		"level": "ignore"
	},
	"no_tabs": {
		"level": "ignore"
	},
	"spacing_after_comma": {
		"level": "ignore"
	}
}
