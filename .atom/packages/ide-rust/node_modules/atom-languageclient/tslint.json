{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "member-ordering": false,
        "one-line": false,
        "interface-name": false,
        "variable-name": false,
        "max-classes-per-file": false,
        "no-unused-expression": false,
        "no-empty": false,
        "one-variable-per-declaration": false,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-separator",
            "check-type",
            "check-typecast",
            "check-module"
        ]
    },
    "rulesDirectory": []
}
